{
  "name": "trap-inventory",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise-grade inventory management system for luxury apparel",
  "scripts": {
    "preinstall": "corepack enable",
    "dev:web": "pnpm --filter @trap/web dev",
    "build:web": "pnpm --filter @trap/web build",
    "lint:web": "pnpm --filter @trap/web lint",
    "dev:api": "cd apps/api && python manage.py runserver 0.0.0.0:8000",
    "migrate": "cd apps/api && python manage.py migrate",
    "makemigrations": "cd apps/api && python manage.py makemigrations"
  },
  "keywords": [
    "inventory",
    "management",
    "luxury",
    "apparel",
    "monorepo"
  ],
  "author": "TRAP",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18",
    "pnpm": "9.x"
  },
  "packageManager": "pnpm@9.15.0"
}
